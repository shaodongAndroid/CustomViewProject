<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_rx"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.custom.view.project.activity.RxActivity">

    <Button
        android:id="@+id/btn_observable"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="observable"
        android:layout_marginTop="2dp"/>
    <Button
        android:id="@+id/btn_just_or_from"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="just_from"
        android:layout_marginTop="2dp"/>
    <Button
        android:id="@+id/btn_scheduler"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="scheduler"
        android:layout_marginTop="2dp"/>
    <Button
        android:id="@+id/btn_map"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="map"
        android:layout_marginTop="2dp"/>
    <ImageView
        android:id="@+id/iv_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <Button
        android:id="@+id/btn_flat_map"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="flat_map"
        android:layout_marginTop="2dp"/>
    <Button
        android:id="@+id/btn_zip"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="zip"
        android:layout_marginTop="2dp"/>
    <Button
        android:id="@+id/btn_zip_1"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="zip_1"
        android:layout_marginTop="2dp"/>
    <Button
        android:id="@+id/btn_flowable"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="flowable"
        android:layout_marginTop="2dp"/>
    <Button
        android:id="@+id/btn_time"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="time"
        android:layout_marginTop="2dp"/>


</LinearLayout>
